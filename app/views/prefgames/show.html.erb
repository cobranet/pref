<%= javascript_include_tag "propulsion_1.2" %>
<%= javascript_include_tag "prefgames" %>
<div>
  <%= image_tag("rails.png") %> 
  <%= link_to_function "Get table data", "cardT.get_table_data()" %>
</div>
<%= content_tag(:div , @prefgame.id, :id => "game_id" ) %>
<div id="jerror"></div>
<div id="log"></div>

<%= form_tag "/prefgames/#{@prefgame.id}/data", remote: true do |f| %>
  <%= text_field_tag :content %>
  <%= submit_tag "Send" %>
<% end %>

<%= content_tag(:div, current_user.id, :id => "user_id") %>
<canvas id="main"></canvas>
<%= subscribe_to "/game/#{@prefgame.id}/#{current_user.id}" %>

